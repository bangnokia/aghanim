{
    "name": "bangnokia/aghanim",
    "description": "A Laravel package for seamless Inertia.js action calls",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Your Name",
            "email": "your.email@example.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0|^11.0",
        "inertiajs/inertia-laravel": "^0.6|^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "BangNokia\\Aghanim\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BangNokia\\Aghanim\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BangNokia\\Aghanim\\AghanimServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}